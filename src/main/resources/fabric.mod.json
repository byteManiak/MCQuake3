{
  "schemaVersion": 1,
  "id": "mcuake",
  "version": "${version}",

  "name": "MCuake",
  "description": "Quake3 mod for Minecraft",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://github.com/bytemaniak",
    "sources": "https://github.com/bytemaniak/MCuake"
  },

  "license": "CC0-1.0",
  "icon": "assets/mcuake/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.bytemaniak.mcuake.MCuake"
    ],
    "client": [
      "com.bytemaniak.mcuake.MCuakeClient"
    ],
    "server": [
      "com.bytemaniak.mcuake.MCuakeServer"
    ]
  },
  "mixins": [
    "mcuake.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.11",
    "fabric-api": "*",
    "minecraft": "~1.19.3",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
