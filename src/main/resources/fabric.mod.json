{
  "schemaVersion": 1,
  "id": "mcquake3",
  "version": "${version}",

  "name": "MCQuake3",
  "description": "Quake3 mod for Minecraft",
  "authors": [
    "byteManiak"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/MCQuake3",
    "sources": "https://github.com/bytemaniak/MCQuake3"
  },

  "license": "CC0-1.0",
  "icon": "assets/mcquake3/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.bytemaniak.mcquake3.MCQuake3"
    ],
    "client": [
      "com.bytemaniak.mcquake3.MCQuake3Client"
    ],
    "preLaunch": [
      "com.bytemaniak.mcquake3.MCQuake3"
    ],
    "emi": [
      "com.bytemaniak.mcquake3.emi.MCQuake3EMI"
