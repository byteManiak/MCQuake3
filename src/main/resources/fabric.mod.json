{
  "schemaVersion": 1,
  "id": "mcquake3",
  "version": "${version}",

  "name": "MCQuake3",
  "description": "Quake3 mod for Minecraft",
  "authors": [
    "byteManiak"
  ],
  "contact": {
    "homepage": "https://github.com/bytemaniak",
    "sources": "https://github.com/bytemaniak/MCQuake3"
  },

  "license": "CC0-1.0",
  "icon": "assets/mcquake3/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.bytemaniak.mcquake3.MCQuake3"
    ],
    "client": [
      "com.bytemaniak.mcquake3.MCQuake3Client"
    ],
    "server": [
      "com.bytemaniak.mcquake3.MCQuake3Server"
    ],
    "preLaunch": [
      "com.bytemaniak.mcquake3.MCQuake3"
    ]
  },
  "mixins": [
    "mcquake3.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.11",
    "fabric-api": "*",
    "minecraft": ">=1.19.3",
    "geckolib": ">=4.0.4",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
